{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017"
    ],
    "allowJs": true,
    "jsx": "react",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strictPropertyInitialization": false,
    "resolveJsonModule": true,
    "noImplicitAny": false
  },
  "include": [
    "index.ts",
    "src/*.ts", "../renderer/src/common/getChildStaticProps.ts", "../../Store.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}