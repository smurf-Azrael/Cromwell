{
  "name": "@cromwell/plugin-newsletter",
  "version": "1.0.4",
  "description": "",
  "files": [
    "build",
    "static",
    "cromwell.config.js"
  ],
  "author": "CromwellJS",
  "license": "MIT",
  "scripts": {
    "build": "npx --no-install cromwell b",
    "watch": "npx --no-install cromwell b -w",
    "migrate": "npx --no-install typeorm migration:generate -f migrationConfig -n init",
    "run:db": "docker run -p 3306:3306 --name mariadb --rm -e MARIADB_ALLOW_EMPTY_ROOT_PASSWORD=true -e MARIADB_DATABASE=cromwell -d mariadb:latest"
  },
  "cromwell": {
    "type": "plugin",
    "title": "Newsletter",
    "description": "Provides a way for web-site visitors to leave thir e-mails for newsletter subscription. E-mails can be CSV exported in Admin panel",
    "icon": "static/icon_email.png",
    "frontendDependencies": [
      "@material-ui/core",
      "@material-ui/lab"
    ]
  },
  "dependencies": {
    "@material-ui/core": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.57"
  },
  "peerDependencies": {
    "@apollo/client": "3.2.1",
    "@cromwell/cli": "^1.0.2",
    "@cromwell/core": "^1.1.3",
    "@cromwell/core-backend": "^1.0.3",
    "@cromwell/core-frontend": "^1.0.2",
    "@cromwell/utils": "^1.0.2",
    "@nestjs/common": "7.6.15",
    "@nestjs/swagger": "4.8.0",
    "@nestjs/throttler": "1.1.3",
    "clsx": "1.1.1",
    "graphql": "15.3.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "tslib": "2.1.0",
    "type-graphql": "1.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.2",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@types/node": "14.0.13",
    "@types/react": "16.9.36",
    "@types/react-dom": "16.9.8",
    "rollup": "^2.28.2",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-ts-compiler": "^1.0.16",
    "typeorm": "0.2.32",
    "typescript": "4.2.4"
  }
}