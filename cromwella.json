{
  "info": "Config for Cromwella package manager",
  "packages": [
    "system/*",
    "system/core/*",
    "plugins/*",
    "themes/*"
  ],
  "frontendDependencies": [
    "@apollo/client",
    "@cromwell/core",
    "@cromwell/core-frontend",
    "@material-ui/lab",
    "@material-ui/core",
    "@material-ui/icons",
    "axios",
    "debounce",
    "gridlex",
    "query-string",
    "react",
    "react-dom",
    "react-number-format",
    "react-html-parser",
    "react-router-dom",
    "react-toastify",
    "reset-css",
    "swiper",
    "tslib",
    {
      "name": "@babel/runtime",
      "addExports": [
        {
          "name": "interopRequireWildcard",
          "path": "@babel/runtime/helpers/interopRequireWildcard",
          "importType": "default",
          "saveAsModules": [
            "@babel/runtime/helpers/interopRequireWildcard"
          ]
        }
      ]
    },
    {
      "name": "next/link",
      "excludeExports": [
        "__esModule"
      ],
      "externals": [
        "react",
        "object-assign",
        "react-is"
      ]
    },
    {
      "name": "next/head",
      "excludeExports": [
        "__esModule"
      ],
      "externals": [
        "react",
        "object-assign",
        "react-is"
      ]
    },
    {
      "name": "next/router",
      "excludeExports": [
        "__esModule"
      ],
      "externals": [
        "react",
        "object-assign",
        "react-is"
      ]
    }
  ]
}