{
    "out": "website/docs/api",
    "exclude": [
        "*.(spec|test).ts",
        "*.(spec|test).tsx",
        "**/es/*",
        "**/dist/*",
        "**/node_modules/*",
        "**/build/*",
        "**/.cromwell/*",
        "**/frontend/src/declarations"
    ],
    "externalPattern": "**/node_modules/**",
    "excludePrivate": true,
    "excludeInternal": true,
    "excludeExternals": true,
    "external-modulemap": ".*system\/core\/([^\/]+)\/.*",
    "entryPoints": [
        "system/core"
    ],
    "tsconfig": "website/apiTsconfig.json"
}