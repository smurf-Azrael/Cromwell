{
    "type": "sqlite",
    "database": "./.cromwell/server/db.sqlite3",
    "synchronize": false,
    "entityPrefix": "crw_",
    "migrationsRun": true,
    "entities": [
        "../core/backend/dist/models/**/*.js"
    ],
    "migrationsTableName": "crw_migrations",
