{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2016",
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017",
      "es2018",
      "esnext.asynciterable"
    ],
    "baseUrl": ".",
    "sourceMap": true,
    "strict": true,
    "emitDecoratorMetadata": true,
    "strictPropertyInitialization": false,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "jsx": "react"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src/**/*.ts", "src/startup.js",
  ],
}